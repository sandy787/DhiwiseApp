<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="barcodesScannerVM" type="com.prajwalsapplication.app.modules.barcodesscanner.data.viewmodel.BarcodesScannerVM"/>
    </data>
    <FrameLayout android:id="@+id/frameBarcodesScanne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageIPhoneXorne"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_21pxh"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="@dimen/_392pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_iphonexorne_black_901"
            tools:ignore="ContentDescription" tools:src="@drawable/img_iphonexorne_black_901"/>
        <FrameLayout android:id="@+id/frameStackgroup9786"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleblack_900_7f">
            <ImageView android:id="@+id/imageGroup9786"
                android:layout_gravity="center"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="@dimen/_325pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group9786"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group9786"/>
            <LinearLayout android:id="@+id/linearColumnvector"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_84pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_84pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtScantheQRcod"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_66pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_15pxh"
                    android:text="@{barcodesScannerVM.barcodesScannerModel.txtScantheQRcod}"
                    style="@style/txtGilroysemibold24_1" tools:text="@string/msg_scan_the_qr_code"/>
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_218pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_73pxv"
                    android:layout_width="@dimen/_325pxh" style="@style/groupStylewhite_A700cornerRadius">
                    <ImageView android:id="@+id/imageImageEightyOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="@dimen/_281pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_image81}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image81"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
